public class DefensivePlayer extends NFLPlayer  {

    private int numOfSacks;
    private int yards; //make sure you change a little bit
    
    public DefensivePlayer(String firstName, String string2, String string3, int i, int j, int k, int e, int numOfSacks, int yards) {
        super(firstName, string2, string3, i, j, k, e);
        this.numOfSacks = numOfSacks;
        this.yards = yards;
    }
    
    public int getNumOfSacks() {
        return this.numOfSacks;
    }
    
    //reminder to make the rest of your getters and setters
    
}