public class DefensivePlayer extends NFLPlayer  {

    private int numOfSacks;
    private int yardsRan; //make sure you change a little bit
    
    public DefensivePlayer(String firstName, String lastName, String team, int age, int weight, int heightFeet, int heightInch, int numOfSacks, int yardsRan) {
        super(firstName, lastName, team, age, weight, heightFeet, heightInch);
        this.numOfSacks = numOfSacks;
        this.yardsRan = yardsRan;
        
      //print our individual player stats
      		System.out.printf(""
      				+ "Defensive Players:"
      				+ "Name: %s %s     "
      				+ "Team: %s      "
      				+ "Age: %d      "
      				+ "Weight: %d      "
      				+ "Height: %d ft. %d in.      " 
      				+ "Number of sacks : %d    " 
      				+ "Yards ran : %d \n",
      				firstName, lastName, team, age, weight, heightFeet, heightInch, numOfSacks, yardsRan);
          }
  
    
    //Getter and Setter
    public int getNumOfSacks() {
        return this.numOfSacks;
    }
    
    public void setNumOfSacks(int  numOfSacks) {
        this.numOfSacks = numOfSacks;
    }
    
    public int getYardsRan() {
        return this.yardsRan;
    }
    
    public void setYardsRan(int yardsRan) {
        this.numOfSacks = yardsRan;
    }
    
}